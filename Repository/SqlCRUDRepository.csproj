<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net40;netcoreapp3.0;netcoreapp3.1</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>2021</Copyright>
    <Authors>Bijan Ghasemi</Authors>
    <Description>Utility for CRUD operation based on Dapper ORM.
Sample Code On Github: https://github.com/Bijan14093/SQLCRUDTest.git
Soruce Code is free on github: https://github.com/Bijan14093/SqlCRUDClass.git</Description>
    <PackageProjectUrl>https://github.com/Bijan14093/SqlCRUDClass.git</PackageProjectUrl>
    <AssemblyVersion>1400.03.18.18</AssemblyVersion>
    <FileVersion>1400.03.18.18</FileVersion>
    <PackageId>SqlCRUDRepository</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIcon>images.jpg </PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageLicenseFile></PackageLicenseFile>
    <Version>1.0.4</Version>
    <PackageTags>SQL ORM Simple CRUD DataBase Save Repository</PackageTags>
    <PackageReleaseNotes>This Version Add Parameter to Find And FindFirst Method for preventing From SqlInjection.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="License.txt" />
    <None Remove="License.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Repository.Domain\SqlCRUDRepository.Domain.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="e:\Git\SqlCRUDClass\images.jpg ">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
